page{
  width: 100%;
  position: absolute;
  z-index: -1;
  background: linear-gradient(180deg, #FFFBFC 10%,#F8C2CF 75%,#FEE3F2 100%);
}
.textPink{
  color: #F48BAD;
  font-size: 30rpx;
}
.topWrap{
  margin-top: 40rpx;
}
.contentBox{
  background: rgba(255,255,255,0.75);
  height: 100rpx;
  width: 100%;
  border-top: 1rpx solid rgba(0, 0, 0, 0.1);
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
}
.contentBox_img{
  position: absolute;
  margin: 30rpx 0 0 35rpx;
  width: 40rpx;
  height: 40rpx;
}
.midWrap{
  margin: 40rpx 0 75rpx 0;
}
.contentBox_input{
  color: rgba(0, 0, 0, 0.6);
  margin: 30rpx 0 0 105rpx;
}
.datePickerText{
  color: #F97390;
  font-weight: 700;
}
.subBtn_add{
  text-align: center;
  margin-left: 27.5%;
  width: 45%;
  padding: 25rpx 0;
  font-size: 26rpx;
  border-radius: 30rpx;
  background: #fff;
  color: #F97390;
  font-weight: 800;
  letter-spacing: 2rpx;
}
.subBtn_alter{
  text-align: center;
  position: absolute;
  right: 12.5%;
  width: 32.5%;
  padding: 25rpx 0;
  font-size: 26rpx;
  border-radius: 30rpx;
  background: #fff;
  color: #F97390;
  font-weight: 800;
  letter-spacing: 2rpx;
}
.delBtn{
  text-align: center;
  position: absolute;
  left: 12.5%;
  width: 32.5%;
  padding: 25rpx 0;
  font-size: 26rpx;
  border-radius: 30rpx;
  background: #F97390;
  color: rgba(0, 0, 0, 0.6);
  font-weight: 800;
  letter-spacing: 2rpx;
}