.topInfoBar{
  text-align: center;
  margin-top: 25rpx;
}
.topInfoBar_logo{
  width: 200rpx;
  height: 200rpx;
}
.topInfoBar_title{
  font-size: 35rpx;
  font-weight: 800;
  color: rgba(0, 0, 0, 1.0);
}
.topInfoBar_detail{
  margin-top: 10rpx;
  font-size: 28rpx;
  font-weight: 700;
  color: rgba(0, 0, 0, 0.8);
}
.topInfoBar_version{
  margin-top: 12rpx;
  font-size: 24rpx;
  color: rgba(0, 0, 0, 0.6);
}
.midWrap{
  margin-top: 50rpx;
}
.aboutBox{
  padding-left: 30rpx;
}
.aboutBox_title{
  font-weight: 700;
  color: rgba(0, 0, 0, 0.8);
  font-size: 30rpx;
}
.aboutBox_detail{
  margin-top: 16rpx;
  color: rgba(0, 0, 0, 0.6);
  font-size: 26rpx;
}
.subBtn{
  text-align: center;
  width: 44%;
  margin-left: 28%;
  padding: 32rpx 0;
  border-radius: 50rpx;
  font-size: 28rpx;
  font-weight: 700;
  color: #709E8D;
  background: rgba(0,0,0,0.05);
  letter-spacing: 1rpx;
}
.groupBox{
  margin-top: 30rpx;
  text-align: center;
  font-size: 30rpx;
  color: rgba(0, 0, 0, 0.5);
}
.groupBox text{
  color: #709E8D;
}
.bottomWrap{
  width: 100%;
  position: absolute;
  text-align: center;
  bottom: 32rpx;
  font-size: 24rpx;
  font-weight: 700;
  color: rgba(0, 0, 0, 0.5);
}