<template name="skeleton">
  <view class="sk-container">
    <view is="miniprogram_npm/@vant/weapp/notify/index" id="van-notify">
      <view is="miniprogram_npm/@vant/weapp/transition/index"></view>
    </view>
    <view style="height:52px;padding-top:50px;">
      <view class="navBar">
        <view class="navBar_title">
          <view class="sk-transparent sk-text-14-2857-623 sk-text" style="background-position-x: 50%;">教务信息</view>
          <text class="courentWeek sk-transparent sk-text-14-2857-256 sk-text" style="background-position-x: 50%;">(未开学)</text>
        </view>
      </view>
    </view>
    <scroll-view class="container" scroll-y="true" style="height: 730px;">
      <view class="topWrap">
        <image class="topSchImg sk-image"></image>
        <view class="topTitle sk-transparent sk-text-14-2857-327 sk-text">选择课程</view>
      </view>
      <view class="course_list">
        <scroll-view scroll-x="true" style="height:76px;white-space:nowrap;">
          <view class="courseBox" data-id="0" style="background:#C1E965">
            <text class="courseBox_title sk-transparent sk-text-14-2857-178 sk-text">JAVA Web框架技术</text>
            <text class="courseBox_class sk-transparent sk-text-14-2857-554 sk-text">20211-4【小2班】</text>
            <text class="courseBox_microNum sk-transparent sk-text-14-2857-772 sk-text">18</text>
          </view>
          <view class="courseBox" data-id="1" style="background:#E3C160">
            <text class="courseBox_title sk-transparent sk-text-14-2857-48 sk-text">Javascript编程技术</text>
            <text class="courseBox_class sk-transparent sk-text-14-2857-953 sk-text">20211-2【小1班】</text>
            <text class="courseBox_microNum sk-transparent sk-text-14-2857-252 sk-text">16</text>
          </view>
          <view class="courseBox" data-id="2" style="background:#EE7785">
            <text class="courseBox_title sk-transparent sk-text-14-2857-9 sk-text">Linux操作系统</text>
            <text class="courseBox_class sk-transparent sk-text-14-2857-791 sk-text">20211-5【小1班】</text>
            <text class="courseBox_microNum sk-transparent sk-opacity">7</text>
          </view>
          <view class="courseBox" data-id="3" style="background:#7BDEE8"></view>
        </scroll-view>
      </view>
      <view class="usefulWrap">
        <view class="usefulWrap_box" data-url="testify">
          <image class="usefulWrap_icon sk-image" style="background: rgba(107,219,246,0.5)"></image>
          <view class="usefulWrap_text sk-transparent sk-text-14-2857-448 sk-text" style="background-position-x: 50%;">加分证明</view>
        </view>
        <view class="usefulWrap_box">
          <image class="usefulWrap_icon sk-image" style="background: rgba(106,91,241,0.5)"></image>
          <view class="usefulWrap_text sk-transparent sk-text-14-2857-671 sk-text" style="background-position-x: 50%;">培养计划</view>
        </view>
        <view class="usefulWrap_box">
          <image class="usefulWrap_icon sk-image" style="background: rgba(252,212,48,0.5)"></image>
          <view class="usefulWrap_text sk-transparent sk-text-14-2857-604 sk-text" style="background-position-x: 50%;">课堂笔记</view>
        </view>
        <view class="usefulWrap_box" style="margin:0;">
          <image class="usefulWrap_icon sk-image" style="background: rgba(122,122,252,0.5)"></image>
          <view class="usefulWrap_text sk-transparent sk-text-14-2857-253 sk-text" style="background-position-x: 50%;">学期目标</view>
        </view>
      </view>
      <view class="infoWrap">
        <view class="infoControlWrap">
          <view class="infoControlBar">
            <image class="infoControlBar_img sk-image" style="background: rgba(112,132,252, 0.5);"></image>
            <view class="infoControlBar_title sk-transparent sk-text-14-2857-117 sk-text">课程信息 08/26</view>
            <view class="infoControlBar_detail sk-transparent sk-text-14-2857-523 sk-text">你今天共有 0 堂课</view>
            <view class="bfBtn sk-transparent sk-text-14-2857-244 sk-text" data-type="before" style="border: none;">前一天</view>
            <view class="afBtn sk-transparent sk-text-14-2857-135 sk-text" data-type="after" style="border: none;">后一天</view>
          </view>
        </view>
        <view class="infoBar">
          <view class="infoListTitleChecked" data-id="0">
            <text class="sk-transparent sk-text-14-2857-244 sk-text">我的课表</text>
            <view class="infoListTitle_border" style="border: none;"></view>
          </view>
          <view class="infoListTitleUnCheck" data-id="1">
            <text class="sk-transparent sk-text-14-2857-840 sk-text">查询成绩</text>
            <view class="infoListTitle_unBorder" style="border: none;"></view>
          </view>
          <view class="infoListTitleUnCheck" data-id="2">
            <text class="sk-transparent sk-text-14-2857-620 sk-text">考试安排</text>
            <view class="infoListTitle_unBorder" style="border: none;"></view>
          </view>
        </view>
        <view class="infoListWrap">
          <scroll-view>
            <view class="defaultWrap">
              <image class="defaultImg sk-image"></image>
              <view class="defaultNotice sk-transparent sk-text-14-2857-140 sk-text" style="background-position-x: 50%;">今天没有课哦</view>
            </view>
          </scroll-view>
        </view>
      </view>
    </scroll-view>
  </view>
</template>